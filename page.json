<script id="exportTableRowTmpl" type="x/text-handlebars-template">
    {{#each rows}}
    <tr>
        <td>
            {{ create_date }}
        </td>
        <td class="cell_wrap">
            {{#if theme_deleted }}
                <span class="spam-label">[{{#translate}}exports_list.label.deleted{{/translate}}]</span>
            {{/if}}
            {{{ theme_name }}}
        </td>
        <td>
            {{ period }}
        </td>
        <td>
            {{#is ext_pic "not" false }}
                <img src="{{ ext_pic }}" alt="{{ ext }}">
            {{/is}}
            {{#is ext "==" "excel"}}
                Excel
            {{ else }}
                {{ toUpperCase ext }}
            {{/is}}
        </td>
        <td class="cell_wrap">
            {{ creator_name }}
        </td>
        <td class="cell_buttons">
            {{#is status "==" 0 }}
                Новый
            {{else is status "==" 1 }}
                Создается...
            {{else is status "==" 3 }}
            <span class="icon-block table_btn disabled_btn" data-id="{{ id }}" title="Произошла ошибка"></span>
                {{#is ../isReaderMode "==" 0 }}
            <span class="icon-delete mod__hover table_btn del_export" data-id="{{ id }}" title="Удалить"></span>
                {{/is}}
            {{else is status "==" 2 }}
                <a class="icon-download mod__hover table_btn ml-0" href="{{ downloadLink }}" title="Скачать"></a>
                {{#is ../isReaderMode "==" 0 }}
                    <span class="icon-delete mod__hover table_btn del_export" data-id="{{ id }}" title="Удалить"></span>
                {{/is}}
            {{ else }}
                <span>Undefined status</span>
            {{/is}}
        </td>
    </tr>
    {{/each}}
</script>




