<section id="logger">
	<script>
  window.addEventListener(
      "error",
      function(r) {
        if(null === r || $.isEmptyObject(r) || (0 == r.lineno && 0 == r.colno) || !r.error || !r.error.stack) return !1;
        if(-1 != ["Script error.", "Script error"].indexOf(r.message)) return !1;
        if(-1 == r.error.stack.indexOf("/app/ba_front/js/")) return !1;
        if(/bot|googlebot|crawler|spider|robot|crawling/i.test(window.navigator.userAgent)) return !1;

        let e = getBrowserVersion();
        if(!Array.prototype.find && -1 == e.indexOf("IE")) return !1;
        let n = r.error.stack.split("\n").filter((r) => !r.includes("jquery")),
            o = {
              'message': r.error.message,
	            'stack': n[1].trim(),
	            'stackInit': n[n.length - 1].trim(),
	            'url': window.location.href,
	            'browser': e,
	            'user': r.currentTarget.ACCOUNT.email,
	            'appVersion': r.currentTarget.localStorage.app_version
            },
            t = new XMLHttpRequest();
        return t.open("POST", "/logger/jserror/", !0), t.send(JSON.stringify(o, null, "\t")), !1;
      },
      !1
  );
</script></section>

